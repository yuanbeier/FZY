@{ 
    Layout = null;
}
@using System.Web.Optimization
@using Abp.Timing
@using Abp.Web.Mvc.Extensions
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=7" />
    <title>J & F Trading</title>
    <meta name="keywords" content="J & F Trading" />
    @Styles.Render("~/Bundles/vendor/css")
    @Scripts.Render("~/Bundles/vendor/js/top")
    <script src="~/js/jquery.nicescroll.js"></script>
    <script type="text/javascript">
        /* 虚拟目录名称 */
        var  virtualDirName = '@ApplicationPath';
    </script>
</head>
<body>
    <div id="all">
        <div class="top">
            <div class="top_middle">
                <div class="search">
               
                </div>

                <div class="lang">
                   
                </div>
            </div>
        </div>

        <div class="conts">
            <div class="aa">
                <img src="~/images/IMG_4510.jpg" style="width:100%;height:100%;position:absolute;">
            </div>
            @Html.Partial("_TopBar")

            <!--[切换图]-->

            <div class="banner">
            </div>

            <!--[切换图]-->
            <!--[内页各部分]-->
            <div class="neiye">
                <div class="neiye_middle">
                    <div class="about_left">
                        <div class="about_title">
                            <p style="position:relative;
							top:10px;">ABOUT US</p>
                            <ul>
                                <li><a href="@Url.Action("Index","About")">>> <span>ABOUT US</span></a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="about_right">
                        <div class="about_right_title">
                            <p>Position：Home > ABOUT US > ABOUT US</p>
                        </div>
                        <div class="contents">

                            <style type="text/css">
                                #sucai {
                                    width: 681px;
                                    height: 542px;
                                    margin: 10px auto;
                                    padding-right: 12px;
                                    overflow: hidden;
                                }


                                    #sucai p {
                                        color: #999;
                                        font-size: 12px;
                                        line-height: 220%;
                                        text-align: left;

                                    }
                            </style>
                            <div id="sucai">
                                <p>
                                    <strong><span style="color:#000000;">Shenzhen J & F Trading Co., Ltd. is a company who provides the most professional packaging solutions eco-friendly.
                                                <br /> We have been shipping our products not only in large quantity with FCL (full container load), but also in  small quantity <br />
                                        with LCL (less container load), which caters various requirements of customers.
</span></strong><br /><br />
                                 
                                    <strong><span style="color:#000000;">Currently, our products range from to go sushi containers, party trays, bento boxes, salad and soup bowls, sauce<br/> containers, etc. with materials of PP, PET, PS, PLA, Paper, Aluminum, etc.
</span></strong><br /><br />
                                    <strong><span style="color:#000000;">In addition, all customized designs are always warmly welcome. What’s more, we promise that all of our productsare <br /> under highly controlled production environment which are able to meet different requirements from our customers.
</span></strong><br /><br />
                                    <strong><span style="color:#000000;">If there is anything we can help, please do not hesitate to contact us. We will serve for your with all sincerity.
</span></strong><br /><br />
                                    @*<strong><span style="color:#000000;">Overview:</span></strong><br />
                                    <br />
                                    <img src="images/about/20150714095634_93378.jpg" alt="" width="300" height="220" title="" align="" /> 
                                    <img src="images/about/20150714095650_14383.jpg" alt="" width="300" height="222" title="" align="" />
                                     <img src="images/about/20150714095713_51278.jpg" alt="" width="300" height="218" title="" align="" /> 
                                    <img src="images/about/20150714095730_24759.jpg" alt="" width="300" height="223" title="" align="" />*@
                                </p>
                            </div>
                        </div>
                        <!--
                        <script type="text/javascript" src="/templets/default/js/jquery.min.js"></script>
                        -->
                        <script type="text/javascript">
									$("#sucai").niceScroll({
										cursorcolor:"#008837",
										cursoropacitymax:1,
										touchbehavior:false,
										cursorwidth:"5px",
										cursorborder:"0",
										cursorborderradius:"5px"
									});
                        </script>
                    </div>

                </div>
            </div>

            <!--[内页各部分]-->


            <div class="clear"></div>
        </div>
        @Html.Partial("_Foot")
    </div>
</body>
</html>