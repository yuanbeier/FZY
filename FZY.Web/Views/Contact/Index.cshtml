@{
    Layout = null;
}
@using System.Web.Optimization
@using Abp.Timing
@using Abp.Web.Mvc.Extensions
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=7" />
    <title>J & F Trading</title>
    <meta name="keywords" content="J & F Trading" />
    @Styles.Render("~/Bundles/vendor/css")
    @Scripts.Render("~/Bundles/vendor/js/top")
</head>
<body>
    <div id="all">
        <div class="top">
            <div class="top_middle">
                <div class="search">
                   
                </div>

                <div class="lang">
                
                </div>
            </div>
        </div>

        <div class="conts">
            <div class="aa">
                <img src="~/images/IMG_4510.jpg" style="width:100%;height:100%;position:absolute;">
            </div>
 
            @Html.Partial("_TopBar")
            <!--[切换图]-->

            <div class="banner">
            </div>

            <!--[切换图]-->
            <!--[内页各部分]-->
            <div class="neiye">
                <div class="neiye_middle">
                    <div class="about_left">
                        <div class="about_title">
                            <p style="position:relative;
							top:10px;">CONTACT US</p>
                            <ul>
                                <li><a href="@Url.Action("index","contact")">>> <span>CONTACT US</span></a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="about_right">
                        <div class="about_right_title">
                            <p>Position：Home > CONTACT US > CONTACT US</p>
                        </div>
                        <div class="contents">
                            <style type="text/css">
                                #sucai {
                                    width: 681px;
                                    height: 600px;
                                    margin: 10px auto;
                                    padding-right: 12px;
                                    overflow: hidden;
                                }

                                    #sucai p {
                                        color: #999;
                                        font-size: 12px;
                                        line-height: 220%;
                                        text-align: left;
                                        text-indent: 2em;
                                    }
                            </style>
                            <div id="sucai">
                                <p>
                                    <span style="color:#000000;"><strong style="font-size: 18px;">Shenzhen J & F Trading Co., Ltd.</strong></span>
                                </p>
                                <p>
                                    <span style="color:#000000;"><strong> Add: Room 1215, Weidonghua Business Building, Qinglong Rd., Longhua Street, </strong></span>
                                </p>
                                <p>
                                    <span style="color:#000000;"><strong>Longhua New Dist., Shenzhen, China</strong></span>
                                </p>
                                <p>
                                    <span style="color:#000000;"><strong>Mob: +86 18025366797 （Fanny)</strong></span>
                                </p>
                                <p>
                                    <span style="color:#000000;"><strong>Email: fanny@jandftrading.com</strong></span>
                                </p>
                         
                                <p>
                                    <span style="color:#000000;"><strong> Website: <a href="http://www.jandftrading.com/">www.jandftrading.com</a></strong></span>
                                </p>
                             
                                <p >
                                    <img src="~/images/weixin.JPG"/>
                                </p>
                            </div>
                        </div>
                        <!--
                        <script type="text/javascript" src="/templets/default/js/jquery.min.js"></script>
                        -->
                        <script src="~/js/jquery.nicescroll.js"></script>
                        <script type="text/javascript">
									$("#sucai").niceScroll({
										cursorcolor:"#008837",
										cursoropacitymax:1,
										touchbehavior:false,
										cursorwidth:"5px",
										cursorborder:"0",
										cursorborderradius:"5px"
									});
                        </script>
                    </div>

                </div>
            </div>

            <!--[内页各部分]-->


            <div class="clear"></div>
        </div>
        @Html.Partial("_Foot")
    </div>
</body>
</html>